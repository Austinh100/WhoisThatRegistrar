{
  "manifest_version": 2,

  "name": "Whois that Registrar",
  "description": "This extension finds the registrar for the current page.",
  "version": "1.0",

  "permissions": [
    "activeTab",
    "*://api.domaintools.com/"
  ],
  "background": {
      "scripts": ["background.js"],
      "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    },
    "default_title": "Whois that Registrar",
    "default_popup": "popup.html"
  }
}